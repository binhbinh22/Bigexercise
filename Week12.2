#include <stdio.h>

void input(double a[], int n)
{
    int i;
    for (i = 0; i < n; i++)
    {
        scanf("%lf", &a[i]);
    }
}

double *maximum(double *a, int n)
{
    int i;
    double *max = a;
    if (n == 0)
        return NULL;
    else
    {
        for (i = 0; i < n; i++)
        {
            if (*(a + i) > *max)
            {
                *max = *(a + i);
            }
        }
        return max;
    }
}

int main()
{
    int n;
    scanf("%d", &n);
    double a[n + 5];
    input(a, n);
    printf("%lf", *maximum(a, n));
    return 0;
}
