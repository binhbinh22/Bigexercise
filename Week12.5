#include <stdio.h>
int n;
void input(int a[], int n)
{
    int i;
    for (i = 0; i < n; i++)
    {
        scanf("%d", &a[i]);
    }
}

void swap(int a[], int n)
{
    int i;
    for (i = n - 1; i >= 0; i--)
    {
        printf("%d ", a[i]);
    }
}

void swap1(int *ptr, int n)
{
    int i;
    for (i = n - 1; i >= 0; i--)
    {
        printf("%d ", *(ptr + i));
    }
}

int main()
{
    scanf("%d", &n);
    int a[n + 5];
    input(a, n);
    swap(a, n);
    printf("\n");
    swap1(a, n);
    return 0;
}
