#include <stdio.h>
#include <string.h>

typedef struct Date
{
    int day;
    int month;
    int year;
} date;

void input(date a[])
{
    int i = 0;
    while (i < 2)
    {
        scanf("%d/%d/%d", &a[i].day, &a[i].month, &a[i].year);
        if (a[i].day > 0 && a[i].day <= 31 && a[i].month > 0 && a[i].month <= 12)
        {
            i++;
        }
        else
        {
            continue;
        }
    }
}

int compare(date a[])
{
    int i = 0;
    if (a[i].year > a[i + 1].year)
    {
        return 1;
    }
    else if (a[i].year < a[i + 1].year)
    {
        return -1;
    }
    else
    {
        if (a[i].month > a[i + 1].month)
        {
            return -1;
        }
        else if (a[i].month < a[i + 1].month)
        {
            return 1;
        }
        else
        {
            if (a[i].day > a[i + 1].day)
            {
                return 1;
            }
            else if (a[i].day < a[i + 1].day)
            {
                return -1;
            }
            else
                return 0;
        }
    }
}

int main()
{
    date a[10];
    input(a);
    printf("%d ", compare(a));
    return 0;
}
